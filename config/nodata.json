{
    "debug": true,
    "http": {
        "enabled": true,
        "listen": "%%NODATA_HTTP%%"
    },
    "query":{
        "connectTimeout": 5000,
        "requestTimeout": 30000,
        "queryAddr": "%%QUERY_HTTP%%"
    },
    "config": {
        "enabled": true,
        "dsn": "%%MYSQL%%/falcon_portal?loc=Local&parseTime=true&wait_timeout=604800",
        "maxIdle": 4
    },
    "collector":{
        "enabled": true,
        "batch": 200,
        "concurrent": 10
    },
    "sender":{
        "enabled": true,
        "connectTimeout": 5000,
        "requestTimeout": 30000,
        "transferAddr": "%%TRANSFER_HTTP%%",
        "batch": 500,
        "block": {
            "enabled": false,
            "threshold": 32
        }
    }
}
